#Making a torus

from turtle import *

for i in range(640):
	circle(200)
	left(53)
	title("Atle Winther - Turtle Graphics")
	forward(i/50+20)
	i += 2
	hideturtle()
	
	
	
	
#Version 2	

for i in range(0, 8000, 6):
	pencolor("turquoise")
	circle(240)
	left(23/100+5)
	title("Atle Winther - Turtle Graphics")
	forward(i/100+5)
	hideturtle()
	speed(60)
	

